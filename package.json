    "build": "tsc && vite build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.2",
    "vite": "^2.2.3"
  },
  "license": "MIT"
}
